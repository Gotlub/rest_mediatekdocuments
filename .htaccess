RewriteEngine on
RewriteRule ^([a-zA-Z]+)$ mediatekdocuments.php?table=$1
RewriteRule ^([a-zA-Z0-9_]+)/([0-9]+)$ mediatekdocuments.php?table=$1&id=$2
RewriteRule ^([a-zA-Z0-9_]+)/({.*})$ mediatekdocuments.php?table=$1&champs=$2
RewriteRule ^([a-zA-Z0-9_]+)/([a-zA-Z0-9_]+)/({.*})$ mediatekdocuments.php?table=$1&id=$2&champs=$3
